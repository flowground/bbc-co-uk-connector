{
    "type": "object",
    "properties": {
        "Authorization": {
            "required": true,
            "type": "string",
            "default": "Bearer OAUTH_TOKEN"
        },
        "type": {
            "required": true,
            "type": "string",
            "enum": [
                "clips",
                "episodes"
            ]
        },
        "pid": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "action": {
                    "type": "string"
                },
                "added_at": {
                    "type": "string"
                },
                "context": {
                    "type": "string"
                },
                "metadata": {
                    "properties": {
                        "key": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "key"
                    ],
                    "type": "object"
                }
            },
            "required": [
                "action"
            ],
            "type": "object"
        },
        "X_Authentication_Provider": {
            "required": true,
            "type": "string",
            "default": "idv5"
        },
        "X_API_Key": {
            "required": true,
            "type": "string"
        }
    }
}