{
    "type": "object",
    "properties": {
        "Authorization": {
            "required": true,
            "type": "string",
            "default": "Bearer OAUTH_TOKEN"
        },
        "type": {
            "required": true,
            "type": "string",
            "enum": [
                "clips",
                "tracks"
            ]
        },
        "action": {
            "required": false,
            "type": "string",
            "enum": [
                "favourited",
                "unfavourited"
            ]
        },
        "offset": {
            "required": false,
            "type": "integer"
        },
        "limit": {
            "required": false,
            "type": "integer"
        },
        "X_Authentication_Provider": {
            "required": true,
            "type": "string",
            "default": "idv5"
        },
        "X_API_Key": {
            "required": true,
            "type": "string"
        }
    }
}